<!DOCTYPE HTML>
<html lang="vi">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="content-language" content="vi" />
    <title><%=news.title_vi%></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="<%= news.description_vi%>" />
    <meta name="keywords" content="Dịch vụ BIM, Scan to BIM, Khảo sát 3D Laser, Số hóa công trình xây dựng" />
    <meta name="author" content="PortcoastDT" />
    <meta name="robots" content="noodp, index, follow">
    <meta name="thumbnail" content="https://portcoast-bim.com/public/images/news/<%= news.folder%>/cover.jpg" />

    <!-- GEO -->
    <meta name="DC.title" lang="vi" content="<%=news.title_vi%>" />
    <meta name="geo.region" content="VN" />
    <meta name="geo.placename" content="Tp. Hồ Chí Minh, Việt Nam" />
    <meta name="DC.identifier" content="https://portcoast-bim.com/vi/tin-tuc/<%= news.url_vi%>" />
    <meta name="geo.position" content="10.798656;106.664696" />
    <meta name="ICBM" content="10.798656, 106.664696" />

    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="<%=news.title_vi%>" />
    <meta property="og:description" content="<%= news.description_vi%>" />
    <meta property="og:image" content="https://portcoast-bim.com/public/images/news/<%= news.folder%>/cover.jpg" />
    <meta property="og:image:width" content="800" />
    <meta property="og:image:height" content="600" />
    <meta property="og:url" content="https://portcoast-bim.com/vi/tin-tuc/<%= news.url_vi%>" />
    <meta property="og:locale" content="vi_VN" />
    <meta property="og:type" content="website" />

    <meta name="twitter:title" content="<%=news.title_vi%>" />
    <meta name="twitter:description" content="<%= news.description_vi%>" />
    <meta name="twitter:image" content="https://portcoast-bim.com/public/images/news/<%= news.folder%>/cover.jpg" />
    <meta name="twitter:url" content="https://portcoast-bim.com/vi/tin-tuc/<%= news.url_vi%>" />
    <%- include (dir+'partials/css.ejs')%>
</head>

<body>
    <div id="pcc-page">

        <%- include (dir+'partials/nav.ejs')%>
        <div id="pcc-main">
            <!-- Page Header Start -->
            <div class="page-header">
                <div class="container">
                    <div class="row">
                    </div>
                </div>
            </div>
            <!-- Page Header End -->
            <section class="pcc-services">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-8">
                            <div class="pcc-narrow-content">
                                <div class="row">
                                    <div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
                                        <span class="heading-meta">
                                            <%=news.tag.split("-")[1]%> |
                                            <%=  [news.day,(news.month<10?`0${news.month}`: news.month),news.year].join("/")%>
                                            <% if(news.tag === "News-Tin Tức" ){%>
                                            | Nguồn: <a href="<%= news.original_url %>">
                                                <%= new URL(news.original_url).hostname.replace('www.','') %>
                                            </a>
                                        <%}%>
                                        </span>
                                        <h2 class="pcc-heading animate-box">
                                            <%= news.title_vi%>
                                        </h2>
                                    </div>
                                </div>
                                <div class="row row-bottom-padded-md">
                                    <div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
                                        <p><strong><%= news.description_vi%></strong></p>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-001.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-002.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-003.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-004.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-005.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-006.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                        <figure>
                                            <img src="/public/images/news/<%= news.folder%>/portcoast-and-pecc2-signed-007.jpg"
                                                alt="<%=news.title_vi%>" class="img-fluid">
                                            <figcaption></figcaption>
                                        </figure>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <%- include(dir+'partials/news.ejs')%>
                    </div>
                </div>
            </section>
            <%- include (dir+'partials/footer.ejs')%>
        </div>
    </div>
    <%- include (dir+'partials/js.ejs')%>
</body>

</html>