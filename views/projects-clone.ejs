<body>
    <div id="pcc-page">
     
        <%- include('partials/nav.ejs') %>

              <div id="pcc-main">
         <!-- Page Header Start -->
            <div class="page-header">
                <div class="container">
                    <div class="row">
                    </div>
                </div>
            </div>
            <!-- Page Header End -->
            <section class="pcc-projects">
                <div class="pcc-narrow-content">
                    <div class="row">
                        <div class="col-md-12 col-md-offset-3 col-md-pull-3 animate-box"
                            data-animate-effect="fadeInLeft">
                            <span class="heading-meta">Projects</span>
                            <h2 class="pcc-heading">Typical Projects</h2>
                        </div>
                    </div>
                    <div class="row row-bottom-padded-md">
                        <div class="filtering col-sm-12 text-center animate-box" data-animate-effect="fadeInLeft">
                            <span style="margin: 15px;" data-filter="*" class="active">All</span>
                            <span style="margin: 15px;" data-filter=".industry" class="">Industry</span>
                            <span style="margin: 15px;" data-filter=".architecture" class="">Architecture</span>
                            <span style="margin: 15px;" data-filter=".port" class="">Ports & Offshore</span>
                            <span style="margin: 15px;" data-filter=".heritage" class="">Heritage</span>
                            <span style="margin: 15px;" data-filter=".infrastructure" class="">Infrastructure</span>
                            <span style="margin: 15px;" data-filter=".interior" class="">Facility Management</span>
                        </div>
                    </div>
                    <div class="row row-bottom-padded-md animate-box" data-animate-effect="fadeInLeft">
                        <div class="gallery">
                            <%for(let i = 0; i < content.length; i++){ %>
                            <div class="col-md-4 col-sm-6 <%= project[i].tags.join(" ") %>">
                                <div class="blog-entry">
                                    <a href="/en/projects/<%= project[i].url_en %>" class=" blog-img"><img
                                            src="/public/images/projects/<%= project[i].folder%>/cover.jpg"
                                            class="img-responsive" alt=""></a>
                                    <div class="desc">
                                        <h3><a href="/en/projects/<%= project[i].url_en %>">
                                                <%= project[i].title_en%></a>
                                        </h3>
                                    </div>
                                </div>
                            </div>
                            <%}%>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
                            <ul class="pagination">
                                <li class="disabled"><a href="#">&laquo;</a></li>
                                <li class="active"><a href="#">1</a></li>
                                <li><a href="#">&raquo;</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
            <%- include ('partials/footer.ejs')%>
                        </div>
                    </div>

                    <%- include('partials/js.ejs') %>

</body>

</html>